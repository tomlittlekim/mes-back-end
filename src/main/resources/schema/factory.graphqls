input FactoryFilter {
    factoryId: String,
    factoryName: String,
    factoryCode: String,
    flagActive: String,
}

type Factory {
    factoryId: String
    factoryName: String
    factoryCode: String
    address: String
    telNo: String
    officerName: String
    flagActive:Boolean
}

type Query {
    factories(filter:FactoryFilter): [Factory]!
}